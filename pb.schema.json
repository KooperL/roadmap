		"system": false,
		"viewQuery": "SELECT \n    (ROW_NUMBER() OVER(ORDER BY c.id)) AS id,   -- universal ID\n    c.id AS card,\n    pr.name as project,\n    t.value AS title,\n    b.value AS body,\n    cc.name AS category,\n    p.value AS priority,\n    ss.name AS status\nFROM card c\n\nJOIN project pr ON c.project = pr.id\n  \n-- Latest title\nLEFT JOIN (\n    SELECT card, MAX(created) AS max_created_at\n    FROM title_tracked\n    GROUP BY card\n) lt ON lt.card = c.id\nLEFT JOIN title_tracked t\n  ON t.card = lt.card\n AND t.created = lt.max_created_at\n\n-- Latest body\nLEFT JOIN (\n    SELECT card, MAX(created) AS max_created_at\n    FROM body_tracked\n    GROUP BY card\n) lb ON lb.card = c.id\nLEFT JOIN body_tracked b\n  ON b.card = lb.card\n AND b.created = lb.max_created_at\n\n-- Latest category\nLEFT JOIN (\n    SELECT card, MAX(created) AS max_created_at\n    FROM category_tracked\n    GROUP BY card\n) lc ON lc.card = c.id\nLEFT JOIN category_tracked cat\n  ON cat.card = lc.card\n AND cat.created = lc.max_created_at\nLEFT JOIN category cc ON cc.id = cat.value\n\n-- Latest priority\nLEFT JOIN (\n    SELECT card, MAX(created) AS max_created_at\n    FROM priority_tracked\n    GROUP BY card\n) lp ON lp.card = c.id\nLEFT JOIN priority_tracked p\n  ON p.card = lp.card\n AND p.created = lp.max_created_at\n\n-- Latest status\nLEFT JOIN (\n    SELECT card, MAX(created) AS max_created_at\n    FROM status_tracked\n    GROUP BY card\n) ls ON ls.card = c.id\nLEFT JOIN status_tracked s\n  ON s.card = ls.card\n AND s.created = ls.max_created_at\nLEFT JOIN status ss ON ss.id = s.value;\n\n"
	}
